programa
{
    funcao inicio()
    {
        inteiro base, expoente, i
        inteiro resultado

   
        escreva("Digite a base: ")
        leia(base)
        escreva("Digite o expoente: ")
        leia(expoente)


        resultado = 1

      
        para (i = 1; i <= expoente; i++)
        {
            resultado = resultado * base
        }

    
        escreva("Resultado de ", base, " elevado a ", expoente, " Ã©: ", resultado)
    }
}
